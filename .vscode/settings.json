{
    "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
    },
    "swift.excludePathsFromPackageUpdates": [
        "app",
        "gradle",
        "**/*.java",
        "**/*.kt",
        "**/*.xml",
        "**/build/**"
    ],
    "swift.sourcekit-lsp.serverPath": "/usr/bin/sourcekit-lsp",
    "swift.path": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin",
    "swift.toolchainPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain",
    "swift.swiftEnvironmentVariables": {
        "DEVELOPER_DIR": "/Applications/Xcode.app/Contents/Developer"
    }
}